{:source-paths ["src"]
 :dependencies [[binaryage/devtools "1.0.2"]
                [nrepl "0.8.3"]
                [reagent "1.0.0"]
                [stylefy "2.2.1"]]
 :builds       {:app {:target     :browser
                      :output-dir "public/js"
                      :asset-path "/js"
                      :modules    {:app {:entries [clojuretodo.core]}}
                      :devtools   {:after-load clojuretodo.core/mount-root}}}

 :dev-http     {3000 {:root    "public"
                      :handler user/app}}}
